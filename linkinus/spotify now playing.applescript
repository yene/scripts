on linkinuscmd(cmd)		if application "Spotify" is not running then		return "/debug echo Spotify is not running."	end if		set AppleScript's text item delimiters to ""	tell application "Spotify"		try			if player state is playing then				set theTrack to name of the current track				set theArtist to artist of the current track				set theAlbum to album of the current track				set theUrl to spotify url of the current track				return "/me is listening to \"" & theTrack & "\"" & " by " & theArtist & ", from " & theAlbum & " " & theUrl			else				return "/debug echo Spotify is paused."			end if		on error			return "/debug echo Error: could not get Song."		end try	end tell	return ""end linkinuscmd